pipeline {
    agent any
    environment {
        BACKEND = 'backend'
        TAG = 'latest'
        RENAME_JAR = 'backend'
        BUILD_CMD = 'bootJar'
        WORKSPACE_DIR = ''
    }
    stages {        
       

        stage('build spring project') {
            steps {
                gitLabCommitStatus(
                    name: 'Deploy to Production',
                    description: 'Deployed to production environment',
                    state: 'success'
                )
                sh 'ls -al'
            
            }
        }

    }
}

